{
  "name": "new-england-keeshonds-app",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/new-england-keeshonds/app/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/vite:build",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/packages/new-england-keeshonds/app"
      },
      "configurations": {
        "development": {
          "mode": "development"
        },
        "production": {
          "mode": "production"
        }
      }
    },
    "serve": {
      "executor": "@nrwl/vite:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "new-england-keeshonds-app:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "new-england-keeshonds-app:build:development",
          "hmr": true
        },
        "production": {
          "buildTarget": "new-england-keeshonds-app:build:production",
          "hmr": false
        }
      }
    },
    "preview": {
      "executor": "@nrwl/vite:preview-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "new-england-keeshonds-app:build"
      },
      "configurations": {
        "development": {
          "buildTarget": "new-england-keeshonds-app:build:development"
        },
        "production": {
          "buildTarget": "new-england-keeshonds-app:build:production"
        }
      }
    },
    "test": {
      "executor": "@nrwl/vite:test",
      "outputs": ["coverage/packages/new-england-keeshonds/app"],
      "options": {
        "passWithNoTests": true,
        "reportsDirectory": "../../../coverage/packages/new-england-keeshonds/app"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/new-england-keeshonds/app/**/*.{ts,tsx,js,jsx}"
        ]
      }
    }
  },
  "tags": ["new-england-keeshonds"]
}
