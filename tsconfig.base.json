{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@daidarabotchi/material-ui": ["libs/material-ui/src/index.ts"],
      "@daidarabotchi/new-england-keeshonds-lib": [
        "libs/new-england-keeshonds-lib/src/index.ts"
      ],
      "@daidarabotchi/portfolio-lib/interfaces": [
        "libs/portfolio-lib/src/interfaces/index.ts"
      ],
      "@daidarabotchi/sephira-front-end-lib": [
        "libs/sephira-front-end-lib/src/index.ts"
      ],
      "@daidarabotchi/sephira-trading-lib": [
        "libs/sephira-trading-lib/src/index.ts"
      ]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
